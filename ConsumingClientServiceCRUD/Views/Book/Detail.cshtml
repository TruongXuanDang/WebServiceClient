
@{
    Layout = null;
    var book = ViewBag.book;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detail</title>
</head>
<body>
    <div>
        <table>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>ISBN</th>
            </tr>
            <tr>
                <td>@book.BookId</td>
                <td>@book.Title</td>
                <td>@book.ISBN</td>
            </tr>
        </table>
</div>
</body>
</html>
