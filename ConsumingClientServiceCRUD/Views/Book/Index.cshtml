
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
        <table>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>ISBN</th>
        </tr>
    @foreach (var item in ViewBag.listBooks)
    {
        <tr>
            <td>@item.BookId</td>
            <td>@item.Title</td>
            <td>@item.ISBN</td>
        </tr>
    }
        </table>
</body>
</html>
